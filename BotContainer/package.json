    "pino-pretty": "^2.6.0",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@types/node": "^11.12.1",
    "@types/shelljs": "^0.8.3",
    "ts-lint": "^4.5.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
